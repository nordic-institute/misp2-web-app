                                    <alias>tomcat-dev-service</alias>
                                    <run>
                                        <ports>
                                            <port>${misp2.development.misp2_port}:8080</port>
                                            <port>${misp2.development.debug_port}:${misp2.development.debug_port}</port>
                                        </ports>
                                        <links>
                                            <link>postgres-dev:db</link>
                                        </links>
                                        <env>
                                            <CATALINA_OPTS>-agentlib:jdwp=transport=dt_socket,server=y,suspend=n,address=${misp2.development.debug_port}</CATALINA_OPTS>
                                        </env>
                                        <labels>
                                            <version>${project.version}</version>
                                            <artifactId>${project.artifactId}</artifactId>
                                        </labels>
                                        <volumes>
                                            <bind>
                                                <volume>${project.build.directory}/${project.build.finalName}:/usr/local/tomcat/webapps/${project.artifactId}</volume>
                                                <volume>${project.basedir}/src/test/docker/entry.sh:/tmp/entry.sh</volume>
                                            </bind>
                                        </volumes>
                                        <cmd>
                                            <shell>/bin/bash -x /tmp/entry.sh run</shell>
                                        </cmd>
                                        <wait>
                                            <http>
                                                <url>http://localhost:${misp2.development.misp2_port}/misp2</url>
                                            </http>
                                            <time>30000</time>
                                        </wait>
                                        <log>
                                            <enabled>true</enabled>
                                        </log>
                                    </run>
                                </image>
                            </images>
                        </configuration>

                        <executions>
