    <profiles>
        <profile>
            <id>it-test</id>
            <properties>
                <JDBC_PORT>${misp2.it-test.jdbc_port}</JDBC_PORT>
                <JDBC_HOST>${misp2.it-test.docker_host_ip}</JDBC_HOST>
            </properties>
            <build>
                <resources>
                    <resource>
                        <directory>${project.basedir}/src/test/resources</directory>
                        <includes>
                            <include>config.cfg</include>
                        </includes>
                        <filtering>true</filtering>
                    </resource>
                    <resource>
                        <directory>${project.basedir}/src/test/resources/certs</directory>
                        <includes>
                            <include>mobiili_id_trust_store.p12</include>
                        </includes>
                        <filtering>false</filtering>
                    </resource>
                </resources>
                <plugins>
                <plugin>
                    <groupId>io.fabric8</groupId>
                    <artifactId>docker-maven-plugin</artifactId>
                    <version>0.34.1</version>
                    <configuration>
                        <containerNamePattern>%a-%i</containerNamePattern>
                        <images>
                            <image>
                                <name>postgres:10-alpine</name>
                                <alias>postgres-test</alias>
                                <run>
                                    <env>
                                        <POSTGRES_PASSWORD>secret</POSTGRES_PASSWORD>
                                        <POSTGRES_HOST_AUTH_METHOD>trust</POSTGRES_HOST_AUTH_METHOD>
                                    </env>
                                    <ports>
                                        <port>${misp2.it-test.jdbc_port}:5432</port>
                                    </ports>
                                    <volumes>
                                        <bind>
                                            ${project.basedir}/src/test/resources/db/initdb.d:/docker-entrypoint-initdb.d
                                        </bind>
                                    </volumes>
                                </run>
                            </image>
                            <image>
                                <name>tomcat:8</name>
                                <alias>tomcat-it-service</alias>
                                <run>
                                    <ports>
                                        <port>${misp2.it-test.misp2_port}:8080</port>
                                    </ports>
                                    <links>
                                        <link>postgres-test:db</link>
                                    </links>
                                    <labels>
                                        <version>${project.version}</version>
                                        <artifactId>${project.artifactId}-it</artifactId>
                                    </labels>
                                    <volumes>
                                        <bind>
                                            <volume>${project.build.directory}/${project.build.finalName}:/usr/local/tomcat/webapps/${project.artifactId}</volume>
                                            <volume>${project.basedir}/src/test/docker/entry.sh:/tmp/entry.sh</volume>
                                        </bind>
                                    </volumes>
                                    <cmd>
                                        <shell>/bin/bash -x /tmp/entry.sh run</shell>
                                    </cmd>
                                    <wait>
                                        <http>
                                            <url>http://localhost:${misp2.it-test.misp2_port}/misp2</url>
                                        </http>
                                        <time>60000</time>
                                    </wait>
                                    <log>
                                        <enabled>true</enabled>
                                    </log>
                                </run>
                            </image>
                        </images>
                    </configuration>
                    <executions>
                        <execution>
                            <id>start-it-db</id>
                            <phase>pre-integration-test</phase>
                            <configuration>
                            </configuration>
                            <goals>
                                <goal>start</goal>
                            </goals>
                        </execution>
                        <execution>
                            <id>stop-it-db</id>
                            <phase>post-integration-test</phase>
                            <configuration>
                                <stopNamePattern>postgres-test**</stopNamePattern>
                                <keepContainer>true</keepContainer>
                            </configuration>
                            <goals>
                                <goal>stop</goal>
                            </goals>
                        </execution>
                    </executions>
                </plugin>
                    <plugin>
                        <groupId>org.codehaus.mojo</groupId>
                        <artifactId>exec-maven-plugin</artifactId>
                        <version>1.6.0</version>
                        <executions>
                            <execution>
                                <id>deploy orbeon</id>
                                <goals>
                                    <goal>exec</goal>
                                </goals>
                                <phase>pre-integration-test</phase>
                            </execution>
                        </executions>
                        <configuration>
                            <executable>${project.basedir}/src/test/docker/deploy_orbeon.sh</executable>
                            <arguments>
                                <argument>tomcat-it-service</argument>
                            </arguments>
                            <workingDirectory>${project.basedir}/src/test/docker/</workingDirectory>
                        </configuration>
                    </plugin>

                <plugin>
                    <artifactId>maven-failsafe-plugin</artifactId>
                    <version>2.22.2</version>
                    <executions>
                        <execution>
                            <goals>
                                <goal>integration-test</goal>
                                <goal>verify</goal>
                            </goals>
                            <configuration>
                                <systemPropertyVariables>
                                    <misp2.it-test.misp2_port>${misp2.it-test.misp2_port}</misp2.it-test.misp2_port>
                                    <misp2.it-test.username>misp2</misp2.it-test.username>
                                    <misp2.it-test.password>secret</misp2.it-test.password>
                                    <misp2.it-test.ss_url>${misp2.it-test.ss_url}</misp2.it-test.ss_url>
                                </systemPropertyVariables>
                                <properties>
                                    <JDBC_PORT>${misp2.it-test.jdbc_port}</JDBC_PORT>
                                </properties>
                            </configuration>
                        </execution>
                    </executions>
                </plugin>
                </plugins>

            </build>
        </profile>
